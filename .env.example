API_PORT=8000
PROGRESS_REPORT_PORT=8080
REVERSE_PROXY_LISTEN=0.0.0.0:80
REACT_APP_PROXY_SERVER="http://api:${API_PORT}"
REACT_APP_PORT=3000
REDIS_DB_SECRET=your_redis_password
ENVIRONMENT=docker

# Updated database URL with encoded password and correct host
GRADESYNC_DATABASE_URL=postgresql://user:password@host:5432/gradesync

# or if connecting via TCP:
# POSTGRES_HOST=your_db_host
# POSTGRES_PORT=5432
# POSTGRES_DB=gradesync
# POSTGRES_USER=postgres
# POSTGRES_PASSWORD=your_db_password

# INSTANCE_CONNECTION_NAME is only needed if connecting via Cloud SQL Proxy
# get this from your Google Cloud Console, format is usually project:region:instance
INSTANCE_CONNECTION_NAME=project:region:instance

# --- from dbcron/.env ---
SERVER_HOST=redis
SERVER_PORT=6379
REDIS_URL=redis://:your_redis_password@redis:6379
SERVER_DBINDEX=0
ASSIGNMENT_CATEGORYROW=2
ASSIGNMENT_CATEGORYCOL=2
ASSIGNMENT_CONCEPTSCOL=2
ASSIGNMENT_CONCEPTSROW=1
ASSIGNMENT_MAXPOINTSROW=3
ASSIGNMENT_MAXPOINTSCOL=2

#BINS INFO
BINS_DBINDEX=1
BINS_COURSE_ID=cs61a-fall-2025
BINS_WORKSHEET=1
BINS_START_ROW=51
BINS_END_ROW=61
BINS_POINTS_COL=0
BINS_GRADES_COL=1

# --- from website/.env.local ---
# REACT_APP_PROXY_SERVER=http://localhost:8000
# REACT_APP_PORT=3000
FAST_REFRESH=true

